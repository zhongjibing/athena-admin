{
    "extends": "@vue/tsconfig/tsconfig.dom.json",
    "include": [
        "env.d.ts",
        "src/**/*",
        "src/**/*.vue",
        "src/**/*.ts",
        "src/**/*.d.ts",
        "src/**/*.vue",
        "auto-imports.d.ts"
    ],
    "exclude": ["src/**/__tests__/*", "node_modules", "dist"],
    "compilerOptions": {
        "composite": true,
        "baseUrl": ".",
        "paths": {
            "@/*": ["./src/*"]
        },
        "moduleResolution": "node"
    }
}
